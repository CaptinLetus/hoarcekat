{
  "name": "hoarcekat",
  "tree": {
    "$className": "DataModel",
    "HttpService": {
      "$className": "HttpService",
      "$properties": {
        "HttpEnabled": true
      }
    },
    "ServerStorage": {
      "$className": "ServerStorage",
      "Hoarcekat": {
        "$className": "Folder",

        "Plugin": {
          "$path": "src"
        },

        "Vendor": {
          "$path": "Packages"
        }
      },
      "Examples": {
        "$path": "examples"
      }
    }
  }
}